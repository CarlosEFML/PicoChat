__lua__
local recvdata={}
local datatosend={}

function initgpio()
  poke(0x5f80,0) // gives ownershipt to javascript
end

function updategpio()
  local gpio0=peek(0x5f80)
  if band(gpio0,0x80)==0 then // owner = javascript
    return
  end

  local rdsz=band(gpio0,0x7f) // get data length to read
  for i=1,rdsz do // read data
    add(recvdata,peek(0x5f80+i))
  end

  local wrsz=min(#datatosend,127) // get data length to write
  for i=1,wrsz do // write data
    local v=deli(datatosend,1)
    poke(0x5f80+i,v)
  end
  poke(0x5f80,wrsz) // gives ownership to javascript and write data size
end

function sendpack(data,isstr)
  local datalen=#data
  if(isstr)data=pack(ord(data,1,datalen))

  add(datatosend,band(datalen,0xff))
  add(datatosend,band(shr(datalen,8),0xff))
  for i=1,datalen do
    add(datatosend,data[i])
  end
end

function recvpack(asstr)
  local recvlen=#recvdata
  if recvlen>1 then
    local pkglen=recvdata[1]+shl(recvdata[2],8)
    if recvlen>=(pkglen+2) then
      deli(recvdata,1)
      deli(recvdata,1)
      local data={}
      for i=1,pkglen do
        add(data,deli(recvdata,1))
      end
      if(asstr)data=chr(unpack(data))
      return data,pkglen
    end
  end
  return nil,-1
end

